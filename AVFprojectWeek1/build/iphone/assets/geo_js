var getGeoLocation=function(e){Ti.Geolocation.locationServicesEnabled?(Ti.Geolocation.accuracy=Ti.Geolocation.ACCURACY_NEAREST_TEN_METERS,Titanium.Geolocation.distanceFilter=1,Ti.Geolocation.getCurrentPosition(function(i){Ti.API.info("gps response: ",JSON.stringify(i)),i.error?Ti.API.error(i.error):i.success&&e({latitude:i.coords.latitude,longitude:i.coords.longitude,accuracy:i.coords.accuracy})})):alert("Location services disabled. Turn on your gps")};exports.getGeoLocation=getGeoLocation;