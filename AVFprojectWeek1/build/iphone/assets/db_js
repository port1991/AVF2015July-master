Ti.Database.install("wg_weather.sqlite","weatherDB").close(),exports.insert=function(e){var i=Ti.Database.open("weatherDB");i.execute("INSERT INTO weather (display_location_full,display_location_city,display_location_zip,display_location_latitude,display_location_longitude,display_location_elevation,observation_time,local_time_rfc822,wind_string,temperature_string,wind_degrees,wind_gust_mph,dewpoint_string,pressure_in,solarradiation,UV,weather,icon_url,temp_f) VALUES (?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?,?)",e.current_observation.display_location.full,e.current_observation.display_location.city,e.current_observation.display_location.zip,e.current_observation.display_location.latitude,e.current_observation.display_location.longitude,e.current_observation.display_location.elevation,e.current_observation.observation_time,e.current_observation.local_time_rfc822,e.current_observation.wind_string,e.current_observation.temperature_string,e.current_observation.wind_degrees,e.current_observation.wind_gust_mph,e.current_observation.dewpoint_string,e.current_observation.pressure_in,e.current_observation.solarradiation,e.current_observation.UV,e.current_observation.weather,e.current_observation.icon_url,e.current_observation.temp_f),i.close()},exports.fetch=function(){var e=Ti.Database.open("weatherDB"),i="SELECT * FROM weather ORDER BY time_added DESC",t=e.execute(i);if(t.isValidRow()){var a={current_observation:{display_location:{full:t.fieldByName("display_location_full"),city:t.fieldByName("display_location_city"),zip:t.fieldByName("display_location_zip"),latitude:t.fieldByName("display_location_latitude"),longitude:t.fieldByName("display_location_longitude"),elevation:t.fieldByName("display_location_elevation")},observation_time:t.fieldByName("observation_time"),local_time_rfc822:t.fieldByName("local_time_rfc822"),wind_string:t.fieldByName("wind_string"),temperature_string:t.fieldByName("temperature_string"),wind_degrees:t.fieldByName("wind_degrees"),wind_gust_mph:t.fieldByName("wind_gust_mph"),dewpoint_string:t.fieldByName("dewpoint_string"),pressure_in:t.fieldByName("pressure_in"),solarradiation:t.fieldByName("solarradiation"),UV:t.fieldByName("UV"),weather:t.fieldByName("weather"),icon_url:t.fieldByName("icon_url"),temp_f:t.fieldByName("temp_f")}};return e.close(),a}};